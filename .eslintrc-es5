{
	"globals": {
	},
    "env": {
        "node": true,
    	"es6": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "rules": {
        "quotes": [2, "single", "avoid-escape"],
        "no-use-before-define": 0,
        "no-underscore-dangle": 0,
        "eol-last": 0,
        "no-trailing-spaces": 2,
        "no-unused-vars": 2,
        "key-spacing": 0
    },
    "plugins": [
        "html"
    ]
}